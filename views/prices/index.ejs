<% layout('layouts/boilerplate') %> 

<a href="/prices/new">New</a>
<h1>Prices</h1>

<% for(let price of prices) {%> 
    
        <ul>
            <li>Date: <%= price.date %> </li>
            <li>Market: <%= price.marketName %> </li>
            <li>Minimum Price: <%= price.minimumPrice %> </li>
            <li>Maximum Price: <%= price.maximumPrice %> </li>
            <li>Average Price: <%= price.averagePrice %> </li>
            <li><a href="/prices/<%= price._id %> ">View</a></li>
            <li><a href="/prices/<%= price._id %>/edit">Edit</a></li>
        </ul>
    <% } %> 